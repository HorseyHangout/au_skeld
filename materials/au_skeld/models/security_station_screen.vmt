"UnlitTwoTexture"
{
	"$basetexture" "au_skeld/models/security_station_screen"
	"$texture2"	"au_skeld/models/security_station_screen_scanline"
	"$surfaceprop" "glass"
	"$selfillum" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.3 0.3 0.3]"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedTextureVar" "$basetexture"
			"animatedTextureFrameNumVar" "$frame"
			"animatedTextureFrameRate" "24"
		}
		 "TextureScroll"
        {
            "texturescrollvar" "$texture2transform"
            "texturescrollrate" 0.075
            "texturescrollangle" -90
        }
	}
}
